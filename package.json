{
  "name": "breast-implant-warranty-frontend",
  "version": "1.0.0",
  "description": "豐胸隆乳矽膠保固查詢系統 - 前端",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:routes": "vite --config vite.config.routes.js",
    "dev:test": "vite --config vite.config.test.js",
    "build": "npx vite build",
    "build:routes": "vite build --config vite.config.routes.js",
    "build:sveltekit": "vite build --config vite.config.sveltekit.js",
    "preview": "vite preview --port 3000",
    "preview:routes": "vite preview --config vite.config.routes.js",
    "preview:sveltekit": "vite preview --config vite.config.sveltekit.js",
    "serve": "vite preview --port 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:sveltekit": "svelte-check --tsconfig ./tsconfig.sveltekit.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "sync": "svelte-kit sync"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/cleave.js": "^1.4.12",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.21",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^6.3.4"
  },
  "keywords": [
    "warranty",
    "medical",
    "frontend",
    "vite",
    "tailwind"
  ],
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "cleave.js": "^1.6.0",
    "flowbite-svelte": "^1.12.5",
    "lucide-svelte": "^0.522.0",
    "qrcode": "^1.5.4",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.8.3",
    "xlsx": "^0.18.5"
  }
}